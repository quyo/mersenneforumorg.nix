###########################################################################
#     Parameter file for Cado-NFS
###########################################################################
# See params/params.c90 for an example which contains some documentation.

###########################################################################
# General parameters
###########################################################################

name = c95

###########################################################################
# Polynomial selection with Kleinjung's algorithm
###########################################################################

tasks.polyselect.degree = 4
tasks.polyselect.P = 7000
tasks.polyselect.admax = 1200
tasks.polyselect.adrange = 72
tasks.polyselect.incr = 12
tasks.polyselect.nq = 16384
tasks.polyselect.nrkeep = 36
tasks.polyselect.ropteffort = 0.9

###########################################################################
# Sieve
###########################################################################

tasks.lim0 = 450000
tasks.lim1 = 550000
tasks.lpb0 = 24
tasks.lpb1 = 25
tasks.sieve.mfb0 = 47
tasks.sieve.mfb1 = 48
tasks.sieve.lambda0 = 1.94
tasks.sieve.lambda1 = 1.91
tasks.sieve.ncurves0 = 7
tasks.sieve.ncurves1 = 8
tasks.I = 11
tasks.sieve.rels_wanted = 2200000
tasks.qmin = 90000
tasks.sieve.qrange = 5000

###########################################################################
# Filtering
###########################################################################

tasks.filter.purge.keep = 150
tasks.filter.target_density = 80.0

###########################################################################
# Linear algebra
###########################################################################

tasks.linalg.bwc.interval = 1500
tasks.linalg.bwc.interleaving = 0
tasks.linalg.m = 64
tasks.linalg.n = 64

###########################################################################
# Characters
###########################################################################

tasks.linalg.characters.nchar = 50
